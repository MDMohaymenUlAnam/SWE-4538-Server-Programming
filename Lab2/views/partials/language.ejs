<div class="w3-container w3-card w3-white">
  <h2 class="w3-text-grey w3-padding-16">
    <i
      class="fa fa-certificate fa-fw w3-margin-right w3-xxlarge w3-text-teal"
    ></i
    >language
  </h2>
  <% for(let i = 0; i<languages.length; i++){ %>
  
    <h5 class="w3-opacity"><b><%= languages[i]["1st"] %></b></h5>
    <div class="w3-light-grey w3-round-xlarge w3-small">
      <div
        class="w3-container w3-center w3-round-xlarge w3-teal"
        style="width: 80%"
      >
        <div class="w3-center w3-text-white">80%</div>
      </div>
    </div>
    <h5 class="w3-opacity"><b><%= languages[i]["2nd"] %></b></h5>
    <div class="w3-light-grey w3-round-xlarge w3-small">
      <div
        class="w3-container w3-center w3-round-xlarge w3-teal"
        style="width: 80%"
      >
        <div class="w3-center w3-text-white">60%</div>
      </div>
    </div>
    <h5 class="w3-opacity"><b><%= languages[i]["3rd"] %></b></h5>
    <div class="w3-container">
      <div class="w3-light-grey w3-round-xlarge w3-small">
        <div
          class="w3-container w3-center w3-round-xlarge w3-teal"
          style="width: 80%"
        >
          <div class="w3-center w3-text-white">20%</div>
        </div>
      </div>
    <hr />
  </div>
  <%}%>
</div>
